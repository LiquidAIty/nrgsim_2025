<!DOCTYPE html>
<html>
  <head>
    <title><%= pkg.name %></title>
    <link rel="stylesheet" type="text/css" href="css/app.css" media="all" />
    <style>
      body {
        background-color: #ffffff;
        margin: 0;
        overflow: hidden;
      }
       
    </style>
  </head>
  <body>
    <section id="controls">
      <div class="control">
        <label for="ventilation-rate">Ventilation rate</label>
        <div id="ventilation-rate"></div>
      </div>
      <div class="control">
        <label for="insulation-level">Insulation level</label>
        <div id="insulation-level"></div>
      </div>
      <div class="control">
        <label for="window-size">Solar facing window size</label>
        <div id="window-size"></div>
      </div>
      <div class="control">
        <label for="window-type">Solar facing window type</label>
        <div id="window-type"></div>
      </div>
      <div class="control">
        <label for="window-shading">Solar window shading</label>
        <div id="window-shading"></div>
      </div>
      <div class="control">
        <button id="save-button">Save</button>
      </div>
    </section>
    <section id="canvas">
    </section>
    <section id="results">
    </section>
    <script type="text/javascript" src="js/app.js"></script>
  </body>
</html>
