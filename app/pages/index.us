<!DOCTYPE html>
<html>
  <head>
    <title><%= pkg.name %></title>
    <link rel="stylesheet" type="text/css" href="css/app.css" media="all" />
    <style>
      body {
        background-color: #ffffff;
        margin: 0;
        overflow: hidden;
      }
       
    </style>
  </head>
  <body>
    <section id="controls">
      <div class="control">
        <label for="Length">Length: <span id="LengthDisp"></span></label>
        <div id="Length"></div>
      </div>
      <div class="control">
        <label for="Depth">Width: <span id="DepthDisp"></span></label>
        <div id="Depth"></div>
      </div>
      <div class="control">
        <label for="Height">Height: <span id="HeightDisp"></span></label>
        <div id="Height"></div>
      </div>
      <hr/>
      <div class="control">
        <label><input type="checkbox" id="NWin">North Windows</input></label>
        <div class="indent">
          <label for="NWinGR">Glazing Ratio: <span id="NWinGRDisp" style="display:none"></span></label>
          <div id="NWinGR"></div>
        </div>
      </div>
      <hr/>
      <div class="control">
        <label><input type="checkbox" id="EWin">East Windows</input></label>
        <div class="indent">
          <label for="EWinGR">Glazing Ratio: <span id="EWinGRDisp" style="display:none"></span></label>
          <div id="EWinGR"></div>
        </div>
      </div>
      <hr/>
            <div class="control">
        <label><input type="checkbox" id="SWin">South Windows</input></label>
        <div class="indent">
          <label for="SWinGR">Glazing Ratio: <span id="SWinGRDisp" style="display:none"></span></label>
          <div id="SWinGR"></div>
        </div>
      </div>
      <hr/>
      <div class="control">
        <label><input type="checkbox" id="WWin">West Windows</input></label>
        <div class="indent">
          <label for="WWinGR">Glazing Ratio: <span id="WWinGRDisp" style="display:none"></span></label>
          <div id="WWinGR"></div>
        </div>
      </div>
      <hr/>
      </div>
      <div class="control">
        <label for="insulation-level">Insulation level</label>
        <div id="insulation-level"></div>
      </div>
      <div class="control">
       <div class="control">
        <label for="ventilation-rate">Ventilation Rate</label>
        <div id="ventilation-rate"></div>
      </div>
        <button id="run-button">Simulate</button>
      </div>
    </section>
    <section id="canvas">
    </section>
    <section id="results">
    </section>
    <script type="text/javascript" src="js/app.js"></script>
  </body>
</html>