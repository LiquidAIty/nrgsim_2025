<!DOCTYPE html>
<html>
  <head>
    <title><%= pkg.name %></title>
    <link rel="stylesheet" type="text/css" href="css/app.css" media="all" />
    <style>
      body {
        background-color: #ffffff;
        margin: 0;
        overflow: hidden;
      }
       
    </style>
  </head>
  <body>
    <section id="controls">
      <div class="control">
        <label for="Length">Length</label>
        <div id="Length"></div>
      </div>
      <div class="control">
        <label for="Depth">Width</label>
        <div id="Depth"></div>
      </div>
      <div class="control">
        <label for="Height">Height</label>
        <div id="Height"></div>
      </div>
      <hr/>
      <div class="control">
        <label><input type="checkbox" id="SWin">South Facing Window</input></label>
        <div class="indent">
          <label for="SWinGR">Window Glazing Ratio</label>
          <div id="SWinGR"></div>
        </div>
      </div>
      <hr/>
      <div class="control">
        <label><input type="checkbox" id="EWin">East Facing Window</input></label>
        <div class="indent">
          <label for="EWinGR">Window Glazing Ratio</label>
          <div id="EWinGR"></div>
        </div>
      </div>
      <hr/>
      <div class="control">
        <label><input type="checkbox" id="NWin">North Facing Window</input></label>
        <div class="indent">
          <label for="NWinGR">Window Glazing Ratio</label>
          <div id="NWinGR"></div>
        </div>
      </div>
      <hr/>
      <div class="control">
        <label><input type="checkbox" id="WWin">West Facing Window</input></label>
        <div class="indent">
          <label for="WWinGR">Window Glazing Ratio</label>
          <div id="WWinGR"></div>
        </div>
      </div>
      <hr/>
      <div class="control">
        <button id="save-button">Save</button>
      </div>
    </section>
    <section id="canvas">
    </section>
    <section id="results">
    </section>
    <script type="text/javascript" src="js/app.js"></script>
  </body>
</html>
