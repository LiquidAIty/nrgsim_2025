            <label>WMO Region</label>
            <select id="continent">
              <option value="WMO_1_Africa">Africa</option>
              <option value="WMO_2_Asia">Asia</option>
              <option value="WMO_3_SouthAmerica">South America</option>
              <option value="WMO_4_NorthAndCentralAmerica">North and Central America</option>
              <option value="WMO_5_SouthwestPacific">Southwest Pacific</option>
              <option value="WMO_6_Europe">Europe</option>
            </select>
          </div>
          <div class="control">
            <label>Country</label>
            <select id="country" disabled="disabled">
            </select>
          </div>

          <div class="control">
            <label>Weather File</label>
            <select id="WeatherFile" disabled="disabled">
            </select>
          </div>
          <div class="control">
            <label>Terrain</label>
            <select id="terrain">
              <option value="city">City</option>
              <option value="suburbs">Suburbs</option>
              <option value="country">Country</option>
              <option value="ocean">Ocean</option>
              <option value="urban">Urban</option>
            </select>
          </div>
        </div>

        <div class="simulation-controls">
          <div class="control">
            <button id="run-button"><%= $.i18n('sim-simulate') %></button>
          </div>
          <div class="control">
            <div id="progressbar" style="display:none"></div>
          </div>
        </div>

        <div id="results">
        </div>

      </div>
    </div>
